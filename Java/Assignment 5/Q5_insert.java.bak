import java.util.Scanner;
class d_insert
{
	Scanner sc=new Scanner(System.in);
	int r;
	int c;
	int [][] arr;
	d_insert(int r,int c){
		this.r=r;
		this.c=c;
		arr=new int[r][c];
	}
	void create(){
		System.out.println("Enter the data in the matrix");
		for (int i=0;i<r ;i++ )
		{
			for(int j=0;j<c;j++){
				arr[i][j]=sc.nextInt();
			}
		}
	}
	
	void print(){
		for (int i=0;i<r ;i++ )
		{
			for(int j=0;j<c;j++){
				System.out.print(arr[i][j]+" ");
			}
			System.out.println();
		}
	}
	
	void insert(d_insert m1,int insi,int insj){
		System.out.println("Insert the data :");
		m1.arr[insi][insj]=sc.nextInt();
		m1.print();
		}
}

class Q5_insert 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the size of array");
		int r=sc.nextInt();
		int c=sc.nextInt();
		d_insert a1=new d_insert(r,c);
		a1.create();
		System.out.println("Enter the index you want to enter the data");
		int insi=sc.nextInt();
		int insj=sc.nextInt();
		a1.insert(a1,insi,insj);
		//System.out.println("Hello World!");
	}
}

